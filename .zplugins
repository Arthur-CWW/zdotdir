#
# .zsh_plugins.txt - antidote plugins file
#

# Clone-only utils
mbadolato/iTerm2-Color-Schemes kind:clone
sindresorhus/pure kind:clone
romkatv/powerlevel10k kind:clone
agkozak/zsh-z kind:clone

# Pre-reqs
sorin-ionescu/prezto path:modules/helper autoload:functions

# my custom plugins
$ZDOTDIR/plugins/git
$ZDOTDIR/plugins/python

# clipboard
ohmyzsh/ohmyzsh path:lib/clipboard.zsh
ohmyzsh/ohmyzsh path:plugins/copybuffer
ohmyzsh/ohmyzsh path:plugins/copyfile
ohmyzsh/ohmyzsh path:plugins/copypath

# editor
ohmyzsh/ohmyzsh path:plugins/fancy-ctrl-z
ohmyzsh/ohmyzsh path:plugins/magic-enter
sorin-ionescu/prezto path:modules/editor

# environment
sorin-ionescu/prezto path:modules/environment
sorin-ionescu/prezto path:runcoms/zprofile

# history
sorin-ionescu/prezto path:modules/history

# macOS
sorin-ionescu/prezto path:modules/osx autoload:functions conditional:is-darwin
sorin-ionescu/prezto path:modules/homebrew

# terminal
sorin-ionescu/prezto path:modules/terminal

# utilities
belak/zsh-utils path:utility
ohmyzsh/ohmyzsh path:plugins/extract
romkatv/zsh-bench kind:path
rupa/z
mattmc3/zman

# completions
sorin-ionescu/prezto path:modules/completion

# deferred plugins
olets/zsh-abbr kind:defer
zdharma-continuum/fast-syntax-highlighting kind:defer
zsh-users/zsh-autosuggestions kind:defer
zsh-users/zsh-history-substring-search kind:defer

# vim: ft=zsh sw=2 ts=2 et
